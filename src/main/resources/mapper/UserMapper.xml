<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.nc7.project00.mapper.UserMapper">
    <insert id="branchAdd" parameterType="com.nc7.project00.entity.User">
        INSERT INTO p00_users(id,username,password,email,phone)
        VALUES
        <foreach collection="params" item="user" separator=",">
            (#{user.id},#{user.username},#{user.password},#{user.email},#{user.phone})
        </foreach>
    </insert>
</mapper>